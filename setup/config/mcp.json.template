{
  "mcpServers": {
    "serena": {
      "command": "__SERENA_CMD__",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena-mcp-server",
        "--context",
        "claude-code"
      ],
      "env": {
        "PATH": "__PATH__"
      }
    },
    "github": {
      "command": "__NPX_CMD__",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "__GITHUB_TOKEN__",
        "PATH": "__PATH__"
      }
    },
    "google-workspace": {
      "command": "__UVX_CMD__",
      "args": [
        "workspace-mcp"
      ],
      "env": {
        "GOOGLE_OAUTH_CLIENT_ID": "__GOOGLE_CLIENT_ID__",
        "GOOGLE_OAUTH_CLIENT_SECRET": "__GOOGLE_CLIENT_SECRET__",
        "USER_GOOGLE_EMAIL": "__GOOGLE_EMAIL__",
        "PATH": "__PATH__"
      }
    },
    "twitter": {
      "command": "__NPX_CMD__",
      "args": [
        "-y",
        "@enescinar/twitter-mcp"
      ],
      "env": {
        "API_KEY": "__TWITTER_API_KEY__",
        "API_SECRET_KEY": "__TWITTER_API_SECRET__",
        "ACCESS_TOKEN": "__TWITTER_ACCESS_TOKEN__",
        "ACCESS_TOKEN_SECRET": "__TWITTER_ACCESS_SECRET__",
        "PATH": "__PATH__"
      }
    },
    "jira": {
      "command": "__JIRA_CMD__",
      "args": [
        "mcp-atlassian"
      ],
      "env": {
        "JIRA_URL": "__JIRA_URL__",
        "JIRA_USERNAME": "__JIRA_USERNAME__",
        "JIRA_API_TOKEN": "__JIRA_API_TOKEN__",
        "PATH": "__PATH__"
      }
    },
    "playwright": {
      "command": "__NPX_CMD__",
      "args": [
        "-y",
        "@playwright/mcp"
      ],
      "env": {
        "PATH": "__PATH__"
      }
    },
    "postgres": {
      "command": "__NPX_CMD__",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "__POSTGRES_URL__"
      ],
      "env": {
        "PATH": "__PATH__"
      }
    },
    "memory": {
      "command": "__NPX_CMD__",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "PATH": "__PATH__"
      }
    },
    "diagram": {
      "command": "__UVX_CMD__",
      "args": [
        "--from",
        "mcp-mermaid-image-gen",
        "mcp_mermaid_image_gen"
      ],
      "env": {
        "PATH": "__PATH__"
      }
    },
    "linkedin": {
      "command": "node",
      "args": [
        "__LINKEDIN_SERVER_PATH__"
      ],
      "env": {
        "LINKEDIN_CLIENT_ID": "__LINKEDIN_CLIENT_ID__",
        "LINKEDIN_CLIENT_SECRET": "__LINKEDIN_CLIENT_SECRET__",
        "LINKEDIN_ACCESS_TOKEN": "__LINKEDIN_ACCESS_TOKEN__",
        "LINKEDIN_API_VERSION": "202302",
        "PATH": "__PATH__"
      }
    }
  }
}
