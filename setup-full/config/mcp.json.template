{
  "mcpServers": {
    "serena": {
      "command": "__SERENA_CMD__",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena-mcp-server",
        "--context",
        "claude-code"
      ],
      "env": {
        "PATH": "__PATH__"
      }
    },
    "github": {
      "command": "__NPX_CMD__",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "__GITHUB_TOKEN__",
        "PATH": "__PATH__"
      }
    },
    "jira": {
      "command": "__JIRA_CMD__",
      "args": [
        "mcp-atlassian"
      ],
      "env": {
        "JIRA_URL": "__JIRA_URL__",
        "JIRA_USERNAME": "__JIRA_USERNAME__",
        "JIRA_API_TOKEN": "__JIRA_API_TOKEN__",
        "PATH": "__PATH__"
      }
    }
  }
}
